{
  "name": "tododeck-api",
  "version": "0.0.0-development",
  "description": "simple restful api for my todo app project written in typescript with express",
  "main": "./dist/index.js",
  "private": true,
  "author": {
    "name": "Bence Markus",
    "email": "markus.bence@icloud.com",
    "url": "https://github.com/mrksbnc/"
  },
  "license": "(MIT OR Apache-2.0)",
  "scripts": {
    "dev": "nodemon",
    "prepare": "husky install",
    "lint": "eslint src/**/*.ts",
    "build": "tsc --build --verbose",
    "format": "eslint src/**/*.ts --fix",
    "start": "tsc --build --verbose && node dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrksbnc/todo-api.git"
  },
  "homepage": "https://github.com/mrksbnc/todo-api#readme",
  "prisma": {
    "schema": "prisma/todo.schema.prisma"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "@mechanicalhuman/bunyan-pretty": "^3.0.3",
    "@prisma/client": "^3.6.0",
    "apicache": "^1.6.3",
    "bcrypt": "^5.0.1",
    "bunyan": "^1.8.15",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "envalid": "^7.2.2",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/apicache": "^1.6.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bunyan": "^1.8.8",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/hpp": "^0.2.2",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/morgan": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prisma": "^3.6.0",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
